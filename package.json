{
  "name": "hikvision-portal-arquivos",
  "version": "1.0.0",
  "description": "Portal de busca e gerenciamento de arquivos de produtos Hikvision",
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "sync": "cd data/products && git pull",
    "push-data": "cd data/products && git add . && git commit -m \"update data\" && git push"
  },
  "keywords": ["hikvision", "nextjs", "portal", "documentos", "produtos"],
  "author": "Hikvision Brasil",
  "license": "ISC",
  "dependencies": {
    "next": "^14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "prettier": "^2.8.8"
  }
}
